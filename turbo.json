{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"],
      "env": ["PLATFORM_SURVEY_API_SECRET"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "dependsOn": ["^build"]
    },
    "test": {},
    "clean": {
      "cache": false
    }
  },
  "globalEnv": ["NODE_ENV", "PAYLOAD_SECRET", "DATABASE_URI", "NEXT_PUBLIC_PAYLOAD_API","PAYLOAD_PUBLIC_SERVER_URL","VERCEL_URL","CMS_URL","CSV_UPLOAD_DIR","MIGRATION_SESSION_API_SECRET","PLATFORM_SURVEY_API_SECRET","NEXT_PUBLIC_DASHBOARD_URL","NEXT_PUBLIC_IMAGE_CHOICE_URL","NEXT_PUBLIC_CONTENT_RANK_URL","NEXT_PUBLIC_SURVEY_URL"]
}
